table Config
  FontName: monospace
  FontSize: 9
  FontColor: #37CFA6
  ToolTip.FontSize: 10
  ToolTip.FontName: gothic
  ToolTip.ForeColor: #0000FF
  ToolTip.BackColor: #FFFFFF
  ToolTip.CaptionOnHover: false
  ToolTip.CaptionPlacement: Right
  Locked: false
  Transparency: 80
  Shadow: true
  ShadowColor: #111
  ShadowX: 1
  ShadowY: 1 
  Bold: false
  ClickDelay: 300
  IconSnap: true
  SnapWidth: 10
  SnapHeight: 10
  SnapOrigin: BottomRight
  SnapShadow: false
  SnapShadowTrans: 200
  CaptionOnHover: true
  CaptionPlacement: bottom
  FillStyle: fillinvert
  Background.Delay: 0
  Background.Source: None
  Background.File: None 
  Background.Mode: Center 
  Background.Color: #C2CCFF
end

table Actions
  Lock: control right doubleClk
  Reload: right doubleClk
  Drag: right hold
  EndDrag: right singleClk
  Execute[0]: left singleClk
  Execute[1]: right singleClk
end

